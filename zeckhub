local lightingBackup = {
	Brightness = Lighting.Brightness,
	Ambient = Lighting.Ambient,
	OutdoorAmbient = Lighting.OutdoorAmbient,
	FogEnd = Lighting.FogEnd,
	Effects = {}
}

-- Guardar efectos existentes
for _, v in pairs(Lighting:GetChildren()) do
	if v:IsA(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({8400,11655,12075,12180,7245,10710,10710,10605,10395,12180})) then
		lightingBackup.Effects[#lightingBackup.Effects+1] = v:Clone()
	end
end

VisualTab:CreateButton({
	Name = TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({25200,16695,15540,13860,3360,8610,10605,12075,12180,10185,11970,12180,3360,10185,11340,11340}),
	Callback = function()
		-- Restaurar propiedades b√°sicas
		Lighting.Brightness = lightingBackup.Brightness
		Lighting.Ambient = lightingBackup.Ambient
		Lighting.OutdoorAmbient = lightingBackup.OutdoorAmbient
		Lighting.FogEnd = lightingBackup.FogEnd

		-- Borrar efectos actuales
		for _, v in pairs(Lighting:GetChildren()) do
			if v:IsA(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({8400,11655,12075,12180,7245,10710,10710,10605,10395,12180})) then
				v:Destroy()
			end
		end

		-- Restaurar efectos originales
		for _, clone in pairs(lightingBackup.Effects) do
			local restored = clone:Clone()
			restored.Parent = Lighting
		end

		Rayfield:Notify({
			Title = TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({6825,11445,10290,11025,10605,11550,12180,3360,11970,10605,12075,12180,10185,11970,12180}),
			Content = TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({10185,11445,10290,11025,10605,11550,12180,3360,11970,10605,12075,12180,10185,11970,12180,4830}),
			Duration = 3,
			Image = TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({11970,10605,10710,11970,10605,12075,10920,4725,10395,12495})
		})
	end
})

