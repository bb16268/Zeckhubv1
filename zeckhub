Players.PlayerRemoving:Connect(function(player)
	for _, beams in pairs(teamBeams) do
		for _, b in pairs(beams) do
			if b.character and Players:GetPlayerFromCharacter(b.character) == player then
				hideBeam(b)
			end
		end
	end
end)
