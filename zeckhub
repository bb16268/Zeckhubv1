local VisualTab = Window:CreateTab(lRIIdqxjwNgLLzzGYeFgOobAYwaieuHHvgPG({'**********************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************************','*************************************************************************************************','************************************************************************************************************'}), lRIIdqxjwNgLLzzGYeFgOobAYwaieuHHvgPG({'*************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'}))
