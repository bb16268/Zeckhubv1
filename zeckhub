local CharTab = Window:CreateTab(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({11445,10185,11025,11550}), TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({12285,12075,10605,11970,4725,11970,11655,12285,11550,10500}))
local Section = CharTab:CreateSection(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({10395,10920,10185,11970,10185,10395,12180,10605,11970,3360,10710,12285,11550,12180,11025,11655,11550,12075}))

local directionalJumpEnabled = true
local moveInAirEnabled = false
local airMovementSpeed = 16
local activeAirBodyVelocity = nil
local cloneESPEnabled = true
local autoTiltEnabled = false
local hotkeyName = TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({9450})
local hotkeyEnum = Enum.KeyCode.Z

local function getGameWalkSpeed()
	local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({7560,12285,11445,10185,11550,11655,11025,10500}))
	return humanoid and humanoid.WalkSpeed or 16
end

local function enableAirMovement(hrp)
	if activeAirBodyVelocity then return end
	local bv = Instance.new(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({6930,11655,10500,12705,9030,10605,11340,11655,10395,11025,12180,12705}))
	bv.MaxForce = Vector3.new(1e5, 0, 1e5)
	bv.Velocity = Vector3.zero
	bv.P = 2500
	bv.Name = TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({6825,11025,11970,7035,11655,11550,12180,11970,11655,11340,9030,10605,11340,11655,10395,11025,12180,12705})
	bv.Parent = hrp
	activeAirBodyVelocity = bv
end

local function disableAirMovement()
	if activeAirBodyVelocity then
		activeAirBodyVelocity:Destroy()
		activeAirBodyVelocity = nil
	end
end

local function setupCharacter(character)
	local humanoid = character:WaitForChild(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({7560,12285,11445,10185,11550,11655,11025,10500}))
	local hrp = character:WaitForChild(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
	airMovementSpeed = getGameWalkSpeed()

	humanoid:GetPropertyChangedSignal(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({7770,12285,11445,11760})):Connect(function()
		if humanoid.Jump then
			if assistAimbotEnabled then
				local target = getClosestEnemy()
				if target and target.Character then
					local tHRP = target.Character:FindFirstChild(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
					if tHRP then
						hrp.CFrame = CFrame.new(hrp.Position, Vector3.new(tHRP.Position.X, hrp.Position.Y, tHRP.Position.Z))
						humanoid.AutoRotate = false
						return
					end
				end
			end

			if directionalJumpEnabled then
				task.defer(function()
					task.wait(0.03)
					local dir = Vector3.new(Camera.CFrame.LookVector.X, 0, Camera.CFrame.LookVector.Z)
					if dir.Magnitude > 0 then
						hrp.CFrame = CFrame.lookAt(hrp.Position, hrp.Position + dir.Unit)
						humanoid.AutoRotate = false
					end
				end)
			else
				humanoid.AutoRotate = true
			end
		end
	end)

	humanoid.StateChanged:Connect(function(_, state)
		if state == Enum.HumanoidStateType.Freefall then
			if moveInAirEnabled then
				enableAirMovement(hrp)
			end
		elseif state == Enum.HumanoidStateType.Landed then
			disableAirMovement()
			humanoid.AutoRotate = true
		end
	end)
end

if LocalPlayer.Character then setupCharacter(LocalPlayer.Character) end
LocalPlayer.CharacterAdded:Connect(setupCharacter)

CharTab:CreateToggle({
	Name = TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({10500,11025,11970,10605,10395,12180,11025,11655,11550,10185,11340,3360,11130,12285,11445,11760}),
	CurrentValue = true,
	Callback = function(val)
		directionalJumpEnabled = val
	end
})

