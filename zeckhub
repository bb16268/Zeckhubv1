				local freeBeam = findFreeBeam(team)
				if freeBeam then
					showBeam(freeBeam, player.Character)
					assignedCount[team] = assignedCount[team] + 1
				end
			elseif hasLine then
				assignedCount[team] = assignedCount[team] + 1
			end
		end
	end
	for _, beams in pairs(teamBeams) do
		for _, b in pairs(beams) do
