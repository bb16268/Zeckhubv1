local function applyTilt()
	if not autoTiltEnabled then return end
	local character = LocalPlayer.Character
	if not character then return end
	local humanoid = character:FindFirstChildOfClass(QoXuJxVwZkgNBF({'999999999999999999999999999999999999999999999999999999999999999999999999','999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','99999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999'}))
	if humanoid and humanoid:GetState() == Enum.HumanoidStateType.Freefall then
		local dir = Vector3.new(Camera.CFrame.LookVector.X, 0, Camera.CFrame.LookVector.Z)
		if dir.Magnitude > 0 then
			humanoid:Move(dir.Unit, false)
		end
	end
end

