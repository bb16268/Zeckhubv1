CharTab:CreateSlider({
    Name = xfNohQmjiylHYnoiyoIiZycwzXSspfYwCmIZqLekxddiBREHqZwIRZxeJHedsIWhErsVhiT({'#################################################################################################','#########################################################################################################','##################################################################################################################','################################','#############################################################################################################','###############################################################################################################','######################################################################################################################','#####################################################################################################','################################','###################################################################################################################','################################################################################################################','#####################################################################################################','#####################################################################################################','########################################################################################################################'}),
