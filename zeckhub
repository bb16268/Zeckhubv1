local CurrentHitboxScale = 5.0
local hitboxEnabled = true

local function findAnyPart(model)
	for _, part in ipairs(model:GetDescendants()) do
		if part:IsA(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({6930,10185,12075,10605,8400,10185,11970,12180})) then
			return part
		end
	end
end

local function createOrUpdateHitboxes(scale)
	for _, model in ipairs(Workspace:GetChildren()) do
		if model:IsA(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({8085,11655,10500,10605,11340})) and model.Name:match(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({9870,7035,7980,7665,7245,8190,8820,9975,6930,6825,7980,7980,9975,3885,10500,4515,3780})) then
			local ball = model:FindFirstChild(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({6930,10185,11340,11340,4830,5040,5040,5145}))
			if not ball then
				local ref = findAnyPart(model)
				if ref then
					ball = Instance.new(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({8400,10185,11970,12180}))
					ball.Name = TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({6930,10185,11340,11340,4830,5040,5040,5145})
					ball.Shape = Enum.PartType.Ball
					ball.Size = Vector3.new(2, 2, 2) * scale
					ball.CFrame = ref.CFrame
					ball.Anchored = true
					ball.CanCollide = false
					ball.Transparency = 0.7
					ball.Material = Enum.Material.ForceField
					ball.Color = Color3.fromRGB(0, 255, 0)
					ball.Parent = model
				end
			else
				ball.Size = Vector3.new(2, 2, 2) * scale
			end
		end
	end
end

local function clearHitboxes()
	for _, model in ipairs(Workspace:GetChildren()) do
		if model:IsA(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({8085,11655,10500,10605,11340})) and model.Name:match(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({9870,7035,7980,7665,7245,8190,8820,9975,6930,6825,7980,7980,9975,3885,10500,4515,3780})) then
			local ball = model:FindFirstChild(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({6930,10185,11340,11340,4830,5040,5040,5145}))
			if ball then ball:Destroy() end
		end
	end
end

workspace.ChildAdded:Connect(function(child)
	if child:IsA(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({8085,11655,10500,10605,11340})) and child.Name:match(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({9870,7035,7980,7665,7245,8190,8820,9975,6930,6825,7980,7980,9975,3885,10500,4515,3780})) then
		task.wait(0.1)
		if hitboxEnabled then
			createOrUpdateHitboxes(CurrentHitboxScale)
		end
	end
end)

GameTab:CreateSlider({
	Name = TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({7560,11025,12180,10290,11655,12600,3360,8715,11025,12810,10605}),
	Range = {0, 20},
	Increment = 0.1,
	Suffix = TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({12600}),
	CurrentValue = CurrentHitboxScale,
	Callback = function(val)
		CurrentHitboxScale = val
		createOrUpdateHitboxes(val)
		Rayfield:Notify({
			Title = TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({8715,11025,12810,10605,3360,7035,10920,10185,11550,10815,10605,10500}),
