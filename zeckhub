	CurrentValue = false,
	Callback = function(val)
		setFullbright(val)
		Rayfield:Notify({
			Title = TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({7350,12285,11340,11340,10290,11970,11025,10815,10920,12180}),
