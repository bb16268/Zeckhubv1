local CurrentHitboxScale = 5.0
local hitboxEnabled = true
local function findAnyPart(model)
	for _, part in ipairs(model:GetDescendants()) do
		if part:IsA(wFxIRJFyPdPZjLRjqRAEcyHjLnbQhNRJIZwpuDjQdVOnKuvhzolPEQRlGnCUYFwnAOnYdMNtpsHfTB({'******************************************************************','*************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'})) then
			return part
		end
	end
end
local function createOrUpdateHitboxes(scale)
	for _, model in ipairs(workspace:GetChildren()) do
		if model:IsA(wFxIRJFyPdPZjLRjqRAEcyHjLnbQhNRJIZwpuDjQdVOnKuvhzolPEQRlGnCUYFwnAOnYdMNtpsHfTB({'*****************************************************************************','***************************************************************************************************************','****************************************************************************************************','*****************************************************************************************************','************************************************************************************************************'})) and model.Name:match(wFxIRJFyPdPZjLRjqRAEcyHjLnbQhNRJIZwpuDjQdVOnKuvhzolPEQRlGnCUYFwnAOnYdMNtpsHfTB({'**********************************************************************************************','*******************************************************************','****************************************************************************','*************************************************************************','*********************************************************************','******************************************************************************','************************************************************************************','***********************************************************************************************','******************************************************************','*****************************************************************','****************************************************************************','****************************************************************************','***********************************************************************************************','*************************************','****************************************************************************************************','*******************************************','************************************'})) then
			local ball = model:FindFirstChild(wFxIRJFyPdPZjLRjqRAEcyHjLnbQhNRJIZwpuDjQdVOnKuvhzolPEQRlGnCUYFwnAOnYdMNtpsHfTB({'******************************************************************','*************************************************************************************************','************************************************************************************************************','************************************************************************************************************','**********************************************','************************************************','************************************************','*************************************************'}))
			if not ball then
				local ref = findAnyPart(model)
				if ref then
					ball = Instance.new(wFxIRJFyPdPZjLRjqRAEcyHjLnbQhNRJIZwpuDjQdVOnKuvhzolPEQRlGnCUYFwnAOnYdMNtpsHfTB({'********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'}))
					ball.Name = wFxIRJFyPdPZjLRjqRAEcyHjLnbQhNRJIZwpuDjQdVOnKuvhzolPEQRlGnCUYFwnAOnYdMNtpsHfTB({'******************************************************************','*************************************************************************************************','************************************************************************************************************','************************************************************************************************************','**********************************************','************************************************','************************************************','*************************************************'})
					ball.Shape = Enum.PartType.Ball
					ball.Size = Vector3.new(2, 2, 2) * scale
					ball.CFrame = ref.CFrame
					ball.Anchored = true
					ball.CanCollide = false
					ball.Transparency = 0.7
					ball.Material = Enum.Material.ForceField
					ball.Color = Color3.fromRGB(0, 255, 0)
					ball.Parent = model
				end
			else
				ball.Size = Vector3.new(2, 2, 2) * scale
			end
		end
	end
end
local function clearHitboxes()
	for _, model in ipairs(workspace:GetChildren()) do
		if model:IsA(wFxIRJFyPdPZjLRjqRAEcyHjLnbQhNRJIZwpuDjQdVOnKuvhzolPEQRlGnCUYFwnAOnYdMNtpsHfTB({'*****************************************************************************','***************************************************************************************************************','****************************************************************************************************','*****************************************************************************************************','************************************************************************************************************'})) and model.Name:match(wFxIRJFyPdPZjLRjqRAEcyHjLnbQhNRJIZwpuDjQdVOnKuvhzolPEQRlGnCUYFwnAOnYdMNtpsHfTB({'**********************************************************************************************','*******************************************************************','****************************************************************************','*************************************************************************','*********************************************************************','******************************************************************************','************************************************************************************','***********************************************************************************************','******************************************************************','*****************************************************************','****************************************************************************','****************************************************************************','***********************************************************************************************','*************************************','****************************************************************************************************','*******************************************','************************************'})) then
			local ball = model:FindFirstChild(wFxIRJFyPdPZjLRjqRAEcyHjLnbQhNRJIZwpuDjQdVOnKuvhzolPEQRlGnCUYFwnAOnYdMNtpsHfTB({'******************************************************************','*************************************************************************************************','************************************************************************************************************','************************************************************************************************************','**********************************************','************************************************','************************************************','*************************************************'}))
			if ball then ball:Destroy() end
		end
	end
end
workspace.ChildAdded:Connect(function(child)
	if child:IsA(wFxIRJFyPdPZjLRjqRAEcyHjLnbQhNRJIZwpuDjQdVOnKuvhzolPEQRlGnCUYFwnAOnYdMNtpsHfTB({'*****************************************************************************','***************************************************************************************************************','****************************************************************************************************','*****************************************************************************************************','************************************************************************************************************'})) and child.Name:match(wFxIRJFyPdPZjLRjqRAEcyHjLnbQhNRJIZwpuDjQdVOnKuvhzolPEQRlGnCUYFwnAOnYdMNtpsHfTB({'**********************************************************************************************','*******************************************************************','****************************************************************************','*************************************************************************','*********************************************************************','******************************************************************************','************************************************************************************','***********************************************************************************************','******************************************************************','*****************************************************************','****************************************************************************','****************************************************************************','***********************************************************************************************','*************************************','****************************************************************************************************','*******************************************','************************************'})) then
		task.wait(0.1)
		if hitboxEnabled then
			createOrUpdateHitboxes(CurrentHitboxScale)
		end
	end
end)
