local BallService = Knit.knit.Services.BallService
if not BallService then return end
local mt = getrawmetatable(game)
local oldNamecall = mt.__namecall

setreadonly(mt, false)

mt.__namecall = newcclosure(function(self, ...)
    if not directionalHitEnabled then
        return oldNamecall(self, ...)
    end
    
    local args = {...}
    local method = getnamecallmethod()

    if method == wFxIRJFyPdPZjLRjqRAEcyHjLnbQhNRJIZwpuDjQdVOnKuvhzolPEQRlGnCUYFwnAOnYdMNtpsHfTB({'*************************************************************************','**************************************************************************************************************','**********************************************************************************************************************','***************************************************************************************************************','***********************************************************************************************************','*****************************************************************************************************','***********************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'}) and tostring(self) == wFxIRJFyPdPZjLRjqRAEcyHjLnbQhNRJIZwpuDjQdVOnKuvhzolPEQRlGnCUYFwnAOnYdMNtpsHfTB({'*************************************************************************','**************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','*************************************************************************************************','***************************************************************************************************','********************************************************************************************************************'}) and self.Parent == BallService.RF then
        local params = args[1]
        if typeof(params) == wFxIRJFyPdPZjLRjqRAEcyHjLnbQhNRJIZwpuDjQdVOnKuvhzolPEQRlGnCUYFwnAOnYdMNtpsHfTB({'********************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}) then
            local lookVector = camera.CFrame.LookVector
            params.LookVector = lookVector
            params.MoveDirection = lookVector
            params.TiltDirection = Vector3.new(lookVector.X, 0, lookVector.Z).Unit

            args[1] = params
            return oldNamecall(self, unpack(args))
        end
    end

    return oldNamecall(self, ...)
end)

