				local target = getClosestEnemy()
				if target and target.Character then
					local tHRP = target.Character:FindFirstChild(QoXuJxVwZkgNBF({'999999999999999999999999999999999999999999999999999999999999999999999999','999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','99999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','9999999999999999999999999999999999999999999999999999999999999999999999999999999999','999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','99999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','99999999999999999999999999999999999999999999999999999999999999999999999999999999','9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','99999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999'}))
