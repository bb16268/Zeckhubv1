	Name = TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({6825,11025,11970,3360,8085,11655,12390,10605,11445,10605,11550,12180,3360,4200,7350,11970,10605,10605,10710,11340,11025,10815,10920,12180,4305}),
	CurrentValue = false,
	Callback = function(val)
		moveInAirEnabled = val
		if not val then disableAirMovement() end
	end
})

