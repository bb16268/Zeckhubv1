	ScreenGui.Name = TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({8820,11025,11340,12180,8820,11655,10815,10815,11340,10605,6930,12285,12180,12180,11655,11550,7455,12285,11025})
	ScreenGui.ResetOnSpawn = false
