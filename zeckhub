	Name = QoXuJxVwZkgNBF({'99999999999999999999999999999999999999999999999999999999999999999','999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','99999999999999999999999999999999','99999999999999999999999999999999999999999999999999999999999999999999999999999','999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','99999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','99999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','99999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','99999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','99999999999999999999999999999999','99999999999999999999999999999999999999999999999999999999999999999999999999999999999','9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','99999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','99999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999'}),
	Range = {0, 100},
	Increment = 1,
	CurrentValue = getGameWalkSpeed(),
	Suffix = QoXuJxVwZkgNBF({'99999999999999999999999999999999','9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','99999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999','99999999999999999999999999999999999999999999999','9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999'}),
	Callback = function(val)
		airMovementSpeed = val
