			Duration = 2,
			Image = val and TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({10710,11340,10185,12075,10920,11340,11025,10815,10920,12180}) or TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({12600})
		})
	end
})

