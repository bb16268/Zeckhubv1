local UserInputService = game:GetService(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({8925,12075,10605,11970,7665,11550,11760,12285,12180,8715,10605,11970,12390,11025,10395,10605}))

if UserInputService.TouchEnabled and not game:GetService(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({7035,11655,11970,10605,7455,12285,11025})):FindFirstChild(TYpGaVmLlmDlqApsUjZybFoptRqoDuYaGDYNlhoVlCCVjzsGuFvYZXip({8820,11025,11340,12180,8820,11655,10815,10815,11340,10605,6930,12285,12180,12180,11655,11550,7455,12285,11025})) then
